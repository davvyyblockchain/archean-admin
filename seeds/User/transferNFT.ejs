<!doctype html>
<html lang="en" class="no-js">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/images/favicon/favicon.ico">
    <script>
        document.documentElement.classList.remove("no-js");
    </script>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
        integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="../assets/css/style.css">
    <link rel="stylesheet" href="../assets/css/responsive.css">
    <link rel="stylesheet" href="../assets/css/slick.css">
    <link rel="stylesheet" href="../assets/css/slick-theme.css">
    <link rel="stylesheet" href="../assets/sass/main.css">
    <title>Transfer NFT | Blockchain Australia Solutions</title>
    <meta name="robots" content="noindex, nofollow">
</head>

<body>
    <div class="overlay"></div>
    <%- include('header'); -%>

    <section class="inner-hero">
        <div class="container">
            <div class="row">
                <div class="col text-center">
                    <h1 class="font-family-infra-extra-bold font-size-42 text-dark-blue">Transfer</h1>
                </div>
            </div>
        </div>
    </section>
    <section class="inner-content sec-padding">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-6">
                    <h5 class="font-family-infra-medium font-size-24 text-color-body">Select NFT & Do Transfer</h5>
                </div>
                <div class="col-6 text-right">
                    <button id="btnTransferTo" data-toggle="modal" data-target="#transferto"
                        class="btn btn-primary btn-peach">
                        Transfer to
                    </button>
                </div>
            </div>
            <div class="row mt-4">
                <div class="col">
                    <div class="row" id="nftlist"> </div>
                    <h4 id="NoNFTData" style="display: none;">No Data Available</h4>
                    <div id="nftLoader" class="loader">Loading...</div>
                    <button onclick="loadData()" id="loadMoreBtn" start='0' type="button" id="loadmore"
                        class="btn btn-outline-cta-peach mx-auto color-white-on-hover" style="display: none;">Load
                        More</button>

                </div>
            </div>
        </div>
    </section>
    <%- include('footer'); -%>

    <!-- Modal -->
    <div class="modal fade" id="transferto" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-md modal-dialog-centered">
            <div class="modal-content">
                <a href="#" class="close-modal" data-dismiss="modal" aria-label="Close">
                    <span class="iconify" data-icon="carbon:close-filled" data-inline="false" style="color: #090928;"
                        data-width="20" data-height="20"></span>
                </a>
                <div class="modal-body">
                    <div class="content-sec">
                        <div class="row">
                            <div class="col">
                                <img src="../assets/images/transfer-icon.svg"
                                    class="img-fluid mx-auto mb-4 mt-4 d-table" />
                                <h6 class="font-family-infra-semibold text-capitalize text-center mb-0">Transfer To</h6>
                            </div>
                        </div>
                        <div class="row align-items-center bg-light-purple pt-3 pb-3 mb-4 mt-4">
                            <div class="col-md-4">
                                <h5 class="font-family-bs-regular text-capitalize mb-0 text-center text-md-left">
                                    Collaborators</h5>
                            </div>
                            <div class="col-md-8">
                                <select name="collaborator" id="collaborator" class="form-control">
                                    <option value=""> Select
                                        Collaborator</option>
                                </select>
                            </div>
                            <div class="text-center mt-1 w-100">
                                <label for="AmountError" id="lblCollaboratorError" class="text-danger d-none"></label>
                            </div>
                        </div>
                        <div class="row align-items-center bg-light-purple pt-3 pb-3 mb-4 mt-4">
                            <div class="col-md-4">
                                <h5 class="font-family-bs-regular text-capitalize mb-0 text-center text-md-left">
                                    Quantity</h5>
                            </div>
                            <div class="col-md-8">
                                <input class="form-control" type="number" min="0" name="nft-quantity" id="nftQuantity"
                                    placeholder="Quantity">
                            </div>
                            <div class="text-center mt-1 w-100">
                                <label for="AmountError" id="lblQuantityError" class="text-danger d-none"></label>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-4">
                        <div class="col text-center">
                            <button id="transferbtn" class="btn btn-primary btn-peach with-icon mx-auto">
                                <span>Transfer</span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
    <!-- Modal -->

    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: jQuery and Bootstrap Bundle (includes Popper) -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.0.0/jquery.min.js"
        integrity="sha512-0nVWK03Ud0k6o8wDkri8jxX9zQIn00ZHVud3iqBTwd2bGFwJDQShGVb3+vX1adCRxQckKQrIQMFmIA3tfWe+Mg=="
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous">
    </script>

    <!-- Option 2: jQuery, Popper.js, and Bootstrap JS
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
    -->
    <script src="https://code.iconify.design/1/1.0.7/iconify.min.js"></script>
    <script type="text/javascript" src="./assets/js/slick.min.js"></script>
    <script type="text/javascript" src="./assets/js/modernizr-custom.js"></script>
    <script type="text/javascript" src="../assets/js/main.js"></script>
    <script src="../User/js/abi.js"></script>
    <script src="../User/js/transferNFT.js"></script>
</body>

</html>