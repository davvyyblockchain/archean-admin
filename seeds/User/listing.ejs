<!doctype html>
<html lang="en" class="no-js">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/images/favicon/favicon.ico">
    <script>
        document.documentElement.classList.remove("no-js");
    </script>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
        integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="../assets/css/style.css">
    <link rel="stylesheet" href="../assets/css/responsive.css">
    <link rel="stylesheet" href="../assets/css/slick.css">
    <link rel="stylesheet" href="../assets/css/slick-theme.css">
    <link rel="stylesheet" href="../assets/sass/main.css">
    <title>Listing | Decrypt NFT</title>
    <meta name="robots" content="noindex, nofollow">
</head>

<body>
    <div class="overlay"></div>
    <%- include('header'); -%>

    <section class="inner-content sec-padding">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-lg-4 col-xl-3 mb-4 mb-md-0">
                    <div class="filterbox">
                        <div class="filterhead d-flex w-100 align-items-center bg-light-purple p-3">
                            <span class="font-family-infra-medium">Filter</span>
                            <span class="iconify" data-icon="bx:bx-filter" data-inline="false" style="color: #17171F;"
                                data-width="18" data-height="18"></span>
                        </div>
                        <div class="filterbody">
                            <div class="row pt-3 pb-3">
                                <div class="col-12 mb-3 mb-lg-0 col-lg-6 text-center">
                                    <button type="button" id="btnFixedPriceFilter"
                                        class="btn btn-outline-cta-peach mx-auto color-white-on-hover">Fixed
                                        Price</button>
                                </div>
                                <div class="col-12 col-lg-6 text-center">
                                    <button type="button" id="btnAuctionFilter"
                                        class="btn btn-outline-cta-peach mx-auto color-white-on-hover">Auction</button>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col">
                                    <div class="accordion" id="accordionExample">
                                        <div class="card">
                                            <div class="card-header" id="headingOne">
                                                <h2 class="mb-0 d-flex align-items-center">
                                                    <button class="btn btn-link btn-block text-left p-3" type="button"
                                                        data-toggle="collapse" data-target="#collapseOne"
                                                        aria-expanded="true" aria-controls="collapseOne">
                                                        <span>By Category</span>
                                                        <span class="iconify" data-icon="bi:chevron-down"
                                                            data-inline="false" style="color: #121212;" data-width="20"
                                                            data-height="20"></span>
                                                    </button>
                                                </h2>
                                            </div>

                                            <div id="collapseOne" class="collapse show" aria-labelledby="headingOne"
                                                data-parent="#accordionExample">
                                                <div class="card-body" id="categoriesPanel">
                                                    <div class="form-group">
                                                        <div class="checkboxf">
                                                            <input type="checkbox" name="category"
                                                                id="chkAllCategory" value="All" checked>
                                                            <span class="checkmark"></span>
                                                            All
                                                        </div>
                                                    </div>
                                                    <!-- <div class="form-group">
                                                        <div class="checkboxf">
                                                            <input type="checkbox" name="category" id="chkArtsCategory"
                                                                value="Arts">
                                                            <span class="checkmark"></span>
                                                            Arts
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <div class="checkboxf">
                                                            <input type="checkbox" name="category" id="chkMusicCategory"
                                                                value="Music">
                                                            <span class="checkmark"></span>
                                                            Music
                                                        </div>
                                                    </div> -->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-8 col-lg-8 col-xl-9">
                    <div class="control-bar">
                        <div class="row align-items-center pt-0">
                            <div class="col-md-7 col-lg-8 col-xl-9 mb-3 mb-md-0">
                                <h3 class="text-dark-blue result-count font-family-infra-medium mb-0" id="totalRecord">
                                </h3>
                            </div>
                            <div class="col-md-5 col-lg-4 col-xl-3">
                                <select class="form-control bas-peach-field font-family-bs-medium text-uppercase" id="sortFilter">
                                    <option value="Recently Added">Recently Added</option>
                                    <option value="Most Viewed">Most Viewed</option>
                                    <option value="Price Low to High">Price Low to High</option>
                                    <option value="Price High to Low">Price High to Low</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-4 has-sidebar">
                        <div class="col">
                            <div class="tab-content" id="myTabContent">
                                <div class="tab-pane fade show active" id="mynft" role="tabpanel"
                                    aria-labelledby="mynft-tab">
                                    <div class="row" id="nftlist"> </div>
                                    <h4 id="NoNFTData" style="display: none;">No Data Available</h4>
                                    <div id="nftLoader" class="loader">Loading...</div>
                                    <button onclick="loadData()" id="loadMoreBtn" start='0' type="button" id="loadmore"
                                        class="btn btn-outline-cta-peach mx-auto color-white-on-hover"
                                        style="display: none;">Load
                                        More</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <%- include('footer'); -%>

    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: jQuery and Bootstrap Bundle (includes Popper) -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.0.0/jquery.min.js"
        integrity="sha512-0nVWK03Ud0k6o8wDkri8jxX9zQIn00ZHVud3iqBTwd2bGFwJDQShGVb3+vX1adCRxQckKQrIQMFmIA3tfWe+Mg=="
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous">
    </script>

    <!-- Option 2: jQuery, Popper.js, and Bootstrap JS
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
    -->
    <script src="https://code.iconify.design/1/1.0.7/iconify.min.js"></script>
    <script type="text/javascript" src="./assets/js/slick.min.js"></script>
    <script type="text/javascript" src="./assets/js/modernizr-custom.js"></script>
    <script type="text/javascript" src="../assets/js/main.js"></script>
    <script src="../User/js/listing.js"></script>
</body>

</html>