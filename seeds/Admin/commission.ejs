<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Manage Commission | Decrypt NFT</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/images/favicon/favicon.ico">
    <link href="../assets/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet">

    <link href="../assets/css/admin-style.css" rel="stylesheet">

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->

    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper" class="show">

        <!--**********************************
            Nav header start
        ***********************************-->

        <!--**********************************
            Nav header end
        ***********************************-->

        <!--**********************************
            Header start
        ***********************************-->
        <%- include('header'); -%>
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

        <!--**********************************
            Sidebar start
        ***********************************-->
        <%- include('sidebar'); -%>
        <!--**********************************
            Sidebar end
        ***********************************-->

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <!-- row -->
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xl-6 col-xxl-12">
                        <div class="row">
                            <div class="col-xl-12 col-lg-12">
                                <div class="card mb-4 mb-xl-0">
                                    <div class="card-header border-0  pb-0">
                                        <div>
                                            <h2 class="text-black fs-20">Commission Percentage</h2>
                                        </div>
                                    </div>
                                    <div class="card-body bg-white border-bottom">
                                        <div class="media profile-bx">
                                            <div class="media-body align-items-center">
                                                <h5 class="text-black">Current Commission Percentage:
                                                    <span id="commissionPercentage">-</span>%</h5>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="col-xl-6 col-xxl-12">
                        <div class="row">
                            <div class="col-xl-12 col-lg-12">
                                <div class="card">
                                    <div class="card-header border-0 d-xl-flex d-lg-block d-md-flex d-sm-flex d-block">
                                        <div class="mr-2">
                                            <h4 class="fs-20 text-black">Update Percentage</h4>
                                        </div>
                                    </div>
                                    <div class="card-body tab-content card-table">
                                        <div class="form-validation">
                                            <span class="form-valide">
                                                <div class="row">
                                                    <div class="col-xl-12">
                                                        <div class="form-group row">
                                                            <label class="col-lg-4 col-form-label"
                                                                for="edit_commission">New Commission Percentage
                                                                <span class="text-danger">*</span>
                                                            </label>
                                                            <div class="col-lg-4">
                                                                <input type="number" class="form-control"
                                                                    id="txtCommission" name="edit_commission"
                                                                    placeholder="Commission %" max="100">
                                                                <div id="commissionPercentage-error"
                                                                    class="invalid-feedback animated fadeInUp">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row">
                                                            <div class="col-lg-8 ml-auto">
                                                                <button type="button" id="btnUpdatePercentage"
                                                                    class="btn btn-primary">Update</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->

        <!--**********************************
            Footer start
        ***********************************-->
        
        <%- include('footer'); -%>

        <!--**********************************
            Footer end
        ***********************************-->

    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->
    <script src="../assets/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="../assets/js/custom.min.js"></script>
    <script src="../assets/js/deznav-init.js"></script>

    <script src="../assets/vendor/web3/web3.min.js"></script>
    <script src="../User/js/abi.js"></script>

    <script src="../Admin/js/commission.js"></script>

</body>

</html>