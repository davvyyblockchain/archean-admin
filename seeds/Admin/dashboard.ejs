<!DOCTYPE html>
<html lang="en">


<!-- xhtml/index.html08:17:22 -->

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width,initial-scale=1">
	<title>Home | Archean</title>

	<!-- Favicon icon -->
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/images/archean-favicon.png">

	<link rel="stylesheet" href="../assets/vendor/chartist/css/chartist.min.css">

	<!-- <link href="css/style.css" rel="stylesheet"> -->
	<link href="../assets/css/admin-style.css" rel="stylesheet">

</head>

<body>

	<!--*******************
        Preloader start
    ********************-->
	<div id="preloader">
		<div class="sk-three-bounce">
			<div class="sk-child sk-bounce1"></div>
			<div class="sk-child sk-bounce2"></div>
			<div class="sk-child sk-bounce3"></div>
		</div>
	</div>
	<!--*******************
        Preloader end
    ********************-->

	<!--**********************************
        Main wrapper start
    ***********************************-->
	<div id="main-wrapper">

		<!-- Include Header -->
		<%- include('header'); -%>

		<!-- Include Sidebar -->
		<%- include('sidebar'); -%>

		<!--**********************************
            Content body start
        ***********************************-->
		<div class="content-body">
			<!-- row -->
			<div class="container-fluid">
				<div class="row">
					<div class="col-xl-12 col-xxl-12 col-lg-12 col-sm-12">
						<div class="card overflow-hidden">
							<div class="card-header border-0 pb-0">
								<div class="mr-auto">
									<h2>Welcome to Archean!!</h2>
								</div>
							</div>
						</div>
					</div><!-- welcome -->
					<div class="col-xl-12 col-xxl-12 col-lg-12 col-sm-12">
						<div class="card overflow-hidden">
							<div class="card-header border-0 pb-0">
								<div class="mr-auto">
									<h2>Total Supply: <span id="totalSupply"></span></h2>
								</div>
							</div>
						</div>
					</div>
					<div class="col-xl-6 col-xxl-6 col-lg-6 col-sm-12">
						<div class="card overflow-hidden p-4">
							<h2>Burn</h2>
							<div class="form-validation">
								<div class="row">
									<div class="col-xl-12">
										<div class="form-group row">
											<label class="col-lg-4 col-form-label"
												for="edit_walletaddress">Serial (Byte32)
											</label>
											<div class="col-lg-6">
												<input type="text" class="form-control"
													id="serial" name="serial"
													placeholder="Serial (Byte32)">
												<small>Please click <a target="_blank" href="https://web3-type-converter.onbrn.com/">here</a> to obtain byte32 hex for your message.</small>
											</div>
										</div>
										<div class="form-group row">
											<div class="col-lg-8 ml-auto">
												<button type="button" id="burnTokens"
													class="btn btn-primary">Burn</button>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-xl-6 col-xxl-6 col-lg-6 col-sm-12">
						<div class="card overflow-hidden p-4">
							<h2>Mint</h2>
							<div class="form-validation">
								<div class="row">
									<div class="col-xl-12">
										<div class="form-group row">
											<label class="col-lg-4 col-form-label"
												for="edit_walletaddress">To 
											</label>
											<div class="col-lg-6">
												<input type="text" class="form-control"
													id="to_wallet_Addresss" name="to_wallet_Addresss"
													placeholder="Wallet Address">
											</div>
										</div>
										<div class="form-group row">
											<label class="col-lg-4 col-form-label"
												for="edit_walletaddress">Serial (Byte32)
											</label>
											<div class="col-lg-6">
												<input type="text" class="form-control"
													id="serial2" name="serial"
													placeholder="Serial (Byte32)">
												<small>Please click <a target="_blank" href="https://web3-type-converter.onbrn.com/">here</a> to obtain byte32 hex for your message.</small>
											</div>
										</div>
										<div class="form-group row">
											<label class="col-lg-4 col-form-label"
												for="edit_walletaddress">Number Of Tokens
											</label>
											<div class="col-lg-6">
												<input type="number" class="form-control"
													id="number_of_tokens" name="number_of_tokens">
											</div>
										</div>
										<div class="form-group row">
											<div class="col-lg-8 ml-auto">
												<button type="button" id="mintTokens"
													class="btn btn-primary">Mint</button>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="card overflow-hidden p-4">
							<h2>Transfer</h2>
							<div class="form-validation">
								<div class="row">
									<div class="col-xl-12">
										<div class="form-group row">
											<label class="col-lg-4 col-form-label"
												for="edit_walletaddress">To 
											</label>
											<div class="col-lg-6">
												<input type="text" class="form-control"
													id="to_wallet_Addresss" name="to_wallet_Addresss"
													placeholder="Wallet Address" max="{{userBalance}}">
													<small id="balancetotransfer"></small>
											</div>
										</div>
										<div class="form-group row">
											<label class="col-lg-4 col-form-label"
												for="edit_walletaddress">Number Of Tokens
											</label>
											<div class="col-lg-6">
												<input type="number" class="form-control"
													id="number_of_tokens" name="number_of_tokens">
											</div>
										</div>
										<div class="form-group row">
											<div class="col-lg-8 ml-auto">
												<button type="button" id="transferTokens"
													class="btn btn-primary">Transfer</button>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>


		<!--**********************************
            Content body end
        ***********************************-->

		<!--**********************************
            Footer start
        ***********************************-->
		
		<%- include('footer'); -%>

		<!--**********************************
            Footer end
        ***********************************-->

	</div>
	<!--**********************************
        Main wrapper end
    ***********************************-->

	<!--**********************************
        Scripts
    ***********************************-->
	<!-- Required vendors -->

	<script src="../assets/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
	<script src="../assets/js/custom.min.js"></script>
	<script src="../assets/js/deznav-init.js"></script>
	<script src="../assets/vendor/chart.js/Chart.bundle.min.js"></script>
	<script src="../assets/vendor/peity/jquery.peity.min.js"></script>

	<script src="../Admin/js/dashboard.js"></script>



</body>

<!-- xhtml/index.html08:18:50 -->

</html>